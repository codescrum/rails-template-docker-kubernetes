{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "redis",
    "labels": {
      "name": "redis",
      "role": "redis"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "redis",
        "image": "redis",
        "ports": [
          {
            "hostPort": 6379,
            "containerPort": 6379
          }
        ]
      }
    ]
  }
}
